EMAIL_VERIFICATION_URL=http://localhost:8080/verify-email
EMAIL_VERIFICATION_TEMPLATE=ProductDB-EmailVerification
RESET_PASSWORD_URL=http://localhost:8080/password
RESET_PASSWORD_TEMPLATE=ProductDB-ResetPassword

# AWS-related

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=

