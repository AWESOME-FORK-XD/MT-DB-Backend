{
  "name": "db-backend",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "startmon": "nodemon ./bin/www",
    "package": "cp .env.$npm_config_env.local .env && sam package --profile $npm_config_profile --template-file ./sam-$npm_config_env-backend.yaml --output-template-file pkg-backend.yaml --resolve-s3",
    "deploy": "sam deploy --profile $npm_config_profile --template-file pkg-backend.yaml --stack-name $npm_config_env-backend --guided",
    "package-test": "npm run package --env=test --profile=medten-test",
    "deploy-test":  "npm run deploy  --env=test --profile=medten-test",
    "package-production": "npm run package --env=production --profile=medten",
    "deploy-production":  "npm run deploy  --env=production --profile=medten"
  },
  "dependencies": {
    "@apigrate/dao": "^5.3.1",
    "@apigrate/logger": "^3.2.0",
    "@apigrate/slack": "^3.0.1",
    "@sendgrid/mail": "^7.2.2",
    "aws-sdk": "^2.953.0",
    "aws-serverless-express": "^3.3.6",
    "bcryptjs": "^2.4.3",
    "commander": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fast-csv": "^4.1.3",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.35",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "request": "^2.88.0",
    "server-timing": "^3.3.2",
    "uuidv4": "^6.2.0"
  }
}
